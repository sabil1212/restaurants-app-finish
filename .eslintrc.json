{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {    
    "no-console": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "prefer-destructuring": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "prefer-template": 0,
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "object-curly-newline": 0,
    "linebreak-style":"off",
    "import/no-cycle": 0
    }
}
